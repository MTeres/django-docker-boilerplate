{
  "name": "sec",
  "version": "0.0.1",
  "description": "Software Engineer Challenge",
  "scripts": {
    "lint:python": "pycodestyle ./apps",
    "test": "echo \"Error: no test specified\" && exit 1",
    "precommit": "npm-run-all lint:python",
    "prepush": "npm-run-all lint:python"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "author": "Marcus Vinicius Carvalho",
  "license": "ISC",
  "homepage": "",
  "eslintConfig": {
    "extends": "airbnb-base",
    "installedESLint": true,
    "plugins": [
      "chai-friendly",
      "import"
    ],
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "no-unused-expressions": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "chai-friendly/no-unused-expressions": 2,
      "valid-jsdoc": [
        "error",
        {
          "requireReturn": false,
          "requireReturnDescription": false
        }
      ],
      "require-jsdoc": [
        "error",
        {
          "require": {
            "FunctionDeclaration": true,
            "MethodDefinition": false,
            "ClassDeclaration": false,
            "ArrowFunctionExpression": false
          }
        }
      ]
    },
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "globals": {
      "window": true,
      "jQuery": true,
      "chai": true,
      "ga": true,
      "expect": true,
      "sinon": true,
      "$": true
    }
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-chai-friendly": "^0.2.0",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.13.3",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.2"
  }
}
